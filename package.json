{
  "name": "facebook-clone",
  "version": "1.0.0",
  "description": "Facebook clone project ",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node ./src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src && webpack --mode development -w",
    "build:assets": "cd src && webpack --mode production",
    "build:server": "babel src --out-dir build",
    "copyAll": "cp -R src/static build && cp -R src/views build && cp -R src/img build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "start": "node build/init.js", 
    "prestart": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JeongWooChan/Facebook_clone.git"
  },
  "author": "JeongWooChan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JeongWooChan/Facebook_clone/issues"
  },
  "homepage": "https://github.com/JeongWooChan/Facebook_clone#readme",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "autoprefixer": "^9.5.1",
    "aws-sdk": "^2.459.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "css-loader": "^2.1.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-mysql-session": "^2.1.0",
    "express-session": "^1.16.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "helmet": "^3.16.0",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "mysql": "^2.17.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.11",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pbkdf2-password": "^1.2.1",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.3",
    "sass-loader": "^7.1.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  }
}
